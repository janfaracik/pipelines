pipeline {
    agent any

    stages {
        stage('Hellaaaao') {
            steps {
                echo 'Hello test two!'
            }
        }
        stage('Jello bello wello mello') {
            steps {
                echo 'Hello World 23'
            }
        }
        stage('Jello bello wello mello cello') {
            steps {
                echo 'Hello World 23'
            }
        }
        stage('PIE') {
            steps {
                echo 'Lots of pie'
            }
        }
        stage('more PIE') {
            steps {
                echo 'Little bit more pie'
            }
        }
    }
}
