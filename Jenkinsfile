pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World!'
            }
        }
        stage('Jello bello wello mello') {
            steps {
                echo 'Hello World 23'
            }
        }
        stage('PIE') {
            steps {
                echo 'Lots of pie'
            }
        }
        stage('more PIE') {
            steps {
                echo 'Little bit more pie'
            }
        }
    }
}
